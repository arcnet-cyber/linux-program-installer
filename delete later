import subprocess
import os

env = os.environ.copy()
env["LANG"] = "C"

result = subprocess.run(
    ["dnf", "search", "gparted"],
    capture_output=True,
    text=True,
    env=env
)

print("Return code:", result.returncode)
print("STDOUT:")
print(result.stdout)
print("STDERR:")
print(result.stderr)
